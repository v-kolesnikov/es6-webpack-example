{
    "extends": "airbnb/base",
    "installedESLint": true,
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error"
    },
    "env": {
        "browser": true,
        "node": true
    }
}
